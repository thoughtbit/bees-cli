{
  "name": "bees-cli",
  "version": "0.5.2",
  "description": "A simple CLI for serve and build web app, support JSON pattern config.",
  "bin": {
    "bee": "./bin/bee"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thoughtbit/bees-cli.git"
  },
  "keywords": [
    "bee",
    "cli",
    "react",
    "create-react-app",
    "webpack"
  ],
  "authors": [
    "moocss <moocss@gmail.com> (https://github.com/moocss)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thoughtbit/bees-cli/issues"
  },
  "homepage": "https://github.com/thoughtbit/bees-cli#readme",
  "files": [
    "bin",
    "src",
    "lib",
    ".eslintrc"
  ],
  "scripts": {
    "build": "rimraf lib && babel src --out-dir lib --presets es2015,stage-0",
    "bee:start": "node ./lib/cli/server.js",
    "bee:build": "node ./lib/cli/build.js",
    "debug": "cross-env NODE_ENV=test mocha --require babel-register --no-timeouts test/**/*-test.js",
    "report": "nyc report --reporter=html",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint --ext .js src",
    "precommit": "npm run lint",
    "pub": "npm publish && git push origin"
  },
  "dependencies": {
    "autoprefixer": "^6.5.4",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "case-sensitive-paths-webpack-plugin": "^1.1.4",
    "chalk": "^1.1.3",
    "chokidar": "^1.6.1",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.26.1",
    "detect-port": "^1.0.7",
    "explain-error": "^1.0.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "friendly-errors-webpack-plugin": "^1.1.2",
    "file-loader": "^0.9.0",
    "filesize": "^3.3.0",
    "fs-extra": "^1.0.0",
    "glob": "^7.1.1",
    "gzip-size": "^3.0.0",
    "http-proxy-middleware": "^0.17.3",
    "is-plain-object": "^2.0.1",
    "json-loader": "^0.5.4",
    "sass-loader": "^4.1.0",
    "node-sass": "^4.1.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "parse-json-pretty": "^0.1.0",
    "postcss": "^5.2.6",
    "postcss-loader": "^1.2.0",
    "react-dev-utils": "^0.4.2",
    "recursive-readdir": "^2.1.0",
    "strip-ansi": "^3.0.1",
    "strip-json-comments": "^2.0.1",
    "style-loader": "^0.13.1",
    "system-bell-webpack-plugin": "^1.0.0",
    "url-loader": "^0.5.7",
    "resolve-url-loader": "^1.6.1",
    "stylus":"^0.54.5",
    "stylus-loader": "^2.4.0",
    "image-webpack-loader": "^3.1.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-uglify-parallel": "^0.1.3",
    "yargs": "^6.5.0"
  },
  "devDependencies": {
    "eslint": "^3.12.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "nodemon": "^1.9.2",
    "expect": "^1.20.2",
    "got": "^6.7.1",
    "husky": "^0.11.9",
    "nyc": "^10.0.0",
    "rimraf": "^2.5.4",
    "cross-env": "^3.1.4",
    "postcss-pxtorem": "^3.3.1",
    "mocha": "^3.2.0"
  },
  "engines": {
    "node": ">=6.9.0"
  }
}
